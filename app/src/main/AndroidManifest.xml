<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ddpl.cipot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logotrans"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logotrans"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ddpl.cipot.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ddpl.cipot.HomeSiswaActivity" />
        <activity android:name="com.ddpl.cipot.ProfilSiswaActivity" />
        <activity android:name="com.ddpl.cipot.IndikatorKemampuanActivity"></activity>
    </application>

</manifest>